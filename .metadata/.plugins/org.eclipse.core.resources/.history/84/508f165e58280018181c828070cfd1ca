package fashionHours.product;

public class Shoes extends Product {

	private int size; 
	
	public Shoes(Brand brand, String description,int size, double price) {
		super(brand, TypeProduct.SHOES, description, price);
		this.size = size;
	}
	
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return super.toString() +  ", size=" + size + "]";
	}


}
