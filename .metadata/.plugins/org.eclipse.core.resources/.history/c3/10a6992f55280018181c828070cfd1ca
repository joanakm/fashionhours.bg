package fashionHours.product;

public class Clothing extends Product {

	public Clothing(Brand brand, String description, SizeClothes sizeClothes, double price) {
		super(brand, TypeProduct.CLOTHES, description, sizeClothes, 0 , price);
	}

	@Override
	public String toString() {
		return "Clothing [toString()=" + super.toString() + "]";
	}

	
}
