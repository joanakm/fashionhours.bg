package fashionHours.product;

public class Clothing extends Product {


	private SizeClothes size;
	
	public Clothing(Brand brand, String description, double price, SizeClothes size) {
		super(brand, TypeProduct.CLOTHES, description, price);
		this.size = size;
		
	}

	@Override
	public String toString() {
		
		return super.toString() + ", size=" + size;
	}



	
	



	
	
}
